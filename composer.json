{
    "name": "nova-kit/setup-nova",
    "description": "Setup basic application with Nova.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://laravel-zero.com",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/log": "^8.0",
        "laravel-zero/framework": "^8.4",
        "symfony/finder": "^5.0",
        "titasgailius/terminal": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "NovaKit\\SetupNova\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["setup-nova"]
}
