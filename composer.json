{
    "name": "nova-kit/nova-installer",
    "description": "Laravel Nova Installer",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/installer": "^5.0",
        "nunomaduro/laravel-console-menu": "^3.4"
    },
    "require-dev": {
        "illuminate/filesystem": "^10.48.2 || ^11.0",
        "illuminate/support": "^10.48.2 || ^11.0",
        "laravel-zero/framework": "^10.0",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^2.0",
        "titasgailius/terminal": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "NovaKit\\SetupNova\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/laravel-nova"]
}
